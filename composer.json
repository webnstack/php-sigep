{
    "name": "tcesarpinho/php-sigep",
    "version": "2.1.0",
    "type": "library",
    "description": "Integração com Web Service do Correios. Gera etiquetas, consulta preços e prazos, imprime etiquetas e PLP, etc.",
    "keywords": ["correios", "sigep"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "require": {
        "php": ">=5.1.0",
        "tcesarpinho/php-sigep-fpdf": ">=0.0.1"
    },
    "replace": {
        "tcesarpinho/php-sigep": ">=0.0.1"
    },
    "autoload": {
        "psr-0": {
            "PhpSigep": "src/"
        },
        "psr-4": {
            "PhpSigep\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rafael Stavarengo",
            "email": "faelsta@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
